    "dotenv": "4.0.0",
    "dotenv-safe": "^4.0.3",
    "express": "4.15.2",
    "glob": "7.1.1",
    "helmet": "3.5.0",
    "morgan": "1.8.1",
    "mysql": "2.13.0",
    "nodemon": "1.11.0",
    "pify": "2.3.0",
    "sequelize": "3.30.4",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0"
  }
}
