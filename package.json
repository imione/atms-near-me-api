{
  "name": "test-api-server",
  "version": "1.0.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "description": "API Test server of shawn",
  "main": "index.js",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "NODE_ENV=development nodemon .",
